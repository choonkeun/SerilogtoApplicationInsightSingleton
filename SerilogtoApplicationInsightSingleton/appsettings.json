{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Information",
      "System": "Information"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Information",
        "System": "Debug"
      }
    }
  },

  "AllowedHosts": "*",

  "AppSettings": {
    "isLocal": true,
    "AzureAppConfigurationConnectionString": "YOUR APPLICATION INSIGHT CONNECTION STRING"
  }
}
